<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.hibernate.beans">
    <class name="Config" table="dic_config">
        <composite-id>   
            <key-property name="id" column="id" type="java.lang.String"/>   
            <key-property name="infName" column="inf_name" type="java.lang.String" />      
        </composite-id> 
        <property name="remark" column="remark" type="java.lang.String" />
        <property name="language" column="language" type="java.lang.String" />
    </class>
</hibernate-mapping>
