<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.hibernate.beans">
    <class name="PerformanceCRM" table="INF_PERFORMANCE_CRM">
        <composite-id>
        	<key-property name="staffId" column="STAFF_ID" type="java.lang.String" />
        	<key-property name="inMonth" column="IN_MONTH" type="java.lang.String" />
        </composite-id>
        <property name="staffName" column="STAFF_NAME" type="java.lang.String" /> 
        <property name="requirmentAnalyze" column="REQUIRMENT_ANALYZE" type="java.lang.Integer" />
        <property name="coding" column="CODING" type="java.lang.Integer" /> 
        <property name="problemCounts" column="PROBLEM_COUNTS" type="java.lang.Integer" /> 
        <property name="checkCode" column="CHECK_CODE" type="java.lang.Integer" /> 
        <property name="codeConfirm" column="CODE_CONFIRM" type="java.lang.Integer" /> 
        <property name="workAttitude" column="WORK_ATTITUDE" type="java.lang.Integer" />
        <property name="training" column="TRAINING" type="java.lang.Integer" /> 
        <property name="teamContribute" column="TEAM_CONTRIBUTE" type="java.lang.Integer" />
        <property name="dailyWork" column="DAILY_WORK" type="java.lang.Integer" />
        <property name="other" column="OTHER" type="java.lang.Integer" />    
    </class>
</hibernate-mapping>